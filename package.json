{
  "name": "ms-vue-admin-build",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "serve": "vite --mode production",
    "build:dev": "npm run typecheck && vite build --mode development",
    "build:test": "vite build --mode test",
    "build:pro": "npm run typecheck && vite build --mode production",
    "report": "rimraf dist && vite build --mode test",
    "typecheck": "tsc --noEmit && vue-tsc --noEmit --skipLibCheck",
    "clean:cache": "rm -rf node_modules && rm -rf .eslintcache && pnpm install",
    "svgo": "svgo -f src/icons/svg -o src/icons/svg",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock,build}/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,css,scss,postcss,less}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "preview": "vite preview --port 8080",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@element-plus/icons-vue": "^2.1.0",
    "@pureadmin/utils": "^1.8.5",
    "@vueuse/core": "^10.1.0",
    "@vueuse/motion": "2.0.0-beta.12",
    "animate.css": "^4.1.1",
    "axios": "^1.3.6",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.2",
    "element-plus": "^2.3.4",
    "js-cookie": "^3.0.5",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "pinia": "^2.0.35",
    "pinia-plugin-persistedstate": "^3.1.0",
    "qrcode": "^1.5.3",
    "qs": "^6.11.1",
    "svgo": "^3.0.2",
    "terser": "^5.17.1",
    "vue": "^3.2.47",
    "vue-i18n": "9",
    "vue-router": "^4.1.6",
    "vue-types": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@iconify-icons/ep": "^1.2.11",
    "@iconify-icons/ri": "^1.2.8",
    "@iconify/vue": "^4.1.1",
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@types/js-cookie": "^3.0.3",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^18.16.1",
    "@types/nprogress": "0.2.0",
    "@types/path-browserify": "^1.0.0",
    "@types/qrcode": "^1.5.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "stylelint": "^15.6.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.3",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mock": "2.9.6",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "1.5.0",
    "web-storage-cache": "^1.1.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "webpack"
      ]
    }
  },
  "engines": {
    "node": ">= 16.13.2"
  },
  "repository": "git@github.com:mashuai333/ms-vue-admin-build.git",
  "license": "MIT"
}
