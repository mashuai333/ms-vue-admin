{
  "name": "ms-vue-admin-build",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "serve": "vite --mode production",
    "build:dev": "npm run typecheck && vite build --mode development",
    "build:test": "vite build --mode test",
    "build:pro": "npm run typecheck && vite build --mode production",
    "report": "rimraf dist && vite build --mode test",
    "typecheck": "tsc --noEmit && vue-tsc --noEmit --skipLibCheck",
    "clean:cache": "rm -rf node_modules && rm -rf .eslintcache && pnpm install",
    "svgo": "svgo -f src/icons/svg -o src/icons/svg",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock,build}/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,css,scss,postcss,less}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "preview": "vite preview --port 8080",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@element-plus/icons-vue": "^2.1.0",
    "@pureadmin/utils": "^1.9.10",
    "@vueuse/core": "^10.5.0",
    "@vueuse/motion": "2.0.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "animate.css": "^4.1.1",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "element-plus": "^2.4.1",
    "js-cookie": "^3.0.5",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "pinia": "^2.0.35",
    "pinia-plugin-persistedstate": "^3.2.0",
    "qrcode": "^1.5.3",
    "qs": "^6.11.2",
    "svgo": "^3.0.2",
    "terser": "^5.23.0",
    "vue": "^3.3.7",
    "vue-i18n": "^9.6.2",
    "vue-router": "^4.2.5",
    "vue-types": "^5.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@iconify-icons/ep": "^1.2.12",
    "@iconify-icons/ri": "^1.2.10",
    "@iconify/vue": "^4.1.1",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@types/js-cookie": "^3.0.5",
    "@types/mockjs": "^1.0.9",
    "@types/node": "^20.8.10",
    "@types/nprogress": "0.2.2",
    "@types/path-browserify": "^1.0.1",
    "@types/qrcode": "^1.5.4",
    "@types/qs": "^6.9.9",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.0",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mock": "3.0.0",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "1.8.22",
    "web-storage-cache": "^1.1.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "webpack"
      ]
    }
  },
  "engines": {
    "node": ">= 16.13.2"
  },
  "repository": "git@github.com:mashuai333/ms-vue-admin-build.git",
  "author": "mas",
  "license": "MIT"
}
